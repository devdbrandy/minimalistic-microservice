{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "mocha": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "rules": {
    "no-unused-vars": 0,
    "no-shadow": 0,
    "no-alert": 0,
    "import/no-cycle": 0,
    "prefer-rest-params": 0,
    "no-restricted-syntax": 0,
    "no-param-reassign": 0,
    "comma-dangle": 0,
    "arrow-parens": 0,
    "no-unused-expressions": 0,
    "object-curly-newline": 0,
    "prefer-template": 0,
    "operator-linebreak": 0
  }
}